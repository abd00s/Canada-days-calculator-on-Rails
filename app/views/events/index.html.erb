<h1>Travel History</h1>
<table>
    	<thead>
    		<tr>
    			<th>From</th><th>To</th><th>Location</th><th>Description</th>
    		</tr>
    	</thead>
      <tbody>
          <% @events.each  do |event| %>
            <tr>
              <td><%= event.date_from  %></td>
              <td><%= event.date_to  %></td>
              <td><%= event.location  %></td>
              <td><%= event.description  %></td>
              </td> 
            </tr> 
          <% end %>
      </tbody>
    </table>
<%= link_to "Home", root_path %>